//.navbar.navbar-default.navbar-fixed-top
.navbar.navbar-default(style="margin-bottom: 0; border-top-width: 0;")
  .container
    .navbar-header
      div.pull-left(style="padding-top: 9px; padding-left: 9px; padding-bottom: 9px;")
        img.navbar-logo.img-circle(src="http://beta.images.coolaj86.com/api/resize/height/50?url=http://dropsha.re/files/nsnOz.8/ajthedj-logo.jpg", style="border: 2px solid white; width: 54px; height: 54px;")
        //-img.navbar-logo.img-circle(src="http://beta.images.coolaj86.com/api/resize/height/250?url=http://dropsha.re/files/WwMgyP8/ajthedj.jpg", style="width: 50px; border: 2px solid white;")
        //-img.navbar-logo.img-circle(src="http://beta.images.coolaj86.com/api/resize/height/250?url=http://dropsha.re/files/Arfxk.8/ajthedj.jpg", style="width: 50px; border: 2px solid white;")
      a.navbar-brand(href="/" style="padding-top: 23px; padding-left: 25px;") AJ the DJ LLC
      div(style="padding-top: 9px;")
        button.navbar-toggle(type='button', ng-init="navCollapsed = true", ng-click="navCollapsed = !navCollapsed")
          span.icon-bar
          span.icon-bar
          span.icon-bar
    #navbar-main.navbar-collapse.collapse(ng-class="!navCollapsed && 'in'")
      ul.nav.navbar-nav(style="padding-top: 9px;")
        //-li.dropdown
          a#themes.dropdown-toggle(data-toggle='dropdown', href='')
            | Themes
            span.caret
          ul.dropdown-menu(aria-labelledby='themes')
            li
              a(href='../default/') Default
            li.divider
            li
              a(href='../amelia/') Amelia
            li
              a(href='../cerulean/') Cerulean
        li(du-scrollspy="about")
          a(ng-href="#about" du-smooth-scroll) About
        li(du-scrollspy="lds")
          a(ng-href="#lds" du-smooth-scroll) LDS Events
        li(du-scrollspy="weddings")
          a(ng-href="#weddings" du-smooth-scroll) Weddings
        //-li(ng-repeat="tab in N.tabs", ng-class="{ active: tab.active }")
          a(ng-href="{{tab.href}}") {{ tab.title }}
      
      ul.nav.navbar-nav.navbar-right
        li
          div(style="padding-top: 16px;")
            button.btn.btn-info(type="button", ng-click="N.bookGig()") Pay Deposit
        //-li(ng-show="!N.session")
          a(href="", ng-click="N.showLoginModal()") Login
        //-li.dropdown(ng-show="N.session")
          a#account.dropdown-toggle(data-toggle='dropdown', href='')
            | Account
            span.caret
          ul.dropdown-menu(aria-labelledby='account')
            li
              a(href="", ui-sref="account") Settings
            li.divider
            li
              a(href="", ng-click="N.logout()") Logout
